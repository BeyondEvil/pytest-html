{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "indent": ["error", 4],
        "require-jsdoc" : 0,
        "semi": ["error", "never"],
        "max-len": ["error", { "code": 120 }],
        "object-curly-spacing": ["error", "always", { "arraysInObjects": true }],
        "camelcase": "off"
    }
}
